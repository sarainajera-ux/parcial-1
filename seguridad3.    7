Algoritmo seguridad3
	secreto<-123456;
	escribir"ingrese codigo";
	leer codigo;
	mientras codigo<>secreto Hacer
		escribir"codigo incorrecto.vuelva a intentar";
		leer codigo;
	FinMientras
	escribir"acceso permitido";
FinAlgoritmo
